{
  "manifest_version": 3,
  "name": "Change TD Color",
  "version": "1.0",
  "description": "特定のtdタグを赤くする拡張機能",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Change TD Color"
  }
}
